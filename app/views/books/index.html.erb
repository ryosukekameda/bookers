<h1>Books</h1>

<% @books.each do |book| %>

<table>
  <thead>
      <tr>
          <th>Title</th>
          <th>Body</th>
          <th colspan="3"></th>
      </tr>
  </thead>
</table>
<tbody>
  <tr>
      <td><%= link_to book.title, "/books/#{book.id}" %></td>
      <td><%= link_to book.body, "/books/#{book.id}" %></td>
      <td><a href="/books/show">show</a></td>
      <td><a href="/books/rdit">edit</a></td>
      <td><a>destroy</a></td>
  </tr>
</tbody>

<% end %>
